        <a class="btn btn-lg btn-primary button" data-toggle="modal" href="#events_top<%= temp['id']; %>">Edit</a>  <a target="_blank" href="/cms/delete/events_top/<%= temp['id'] %>" class="btn btn-lg btn-danger button">Delete</a>
  </div>
</div>

<%
  temp = [];
})
%>

</div>
