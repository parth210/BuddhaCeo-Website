            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form action="/cms/home/contact_top" method="POST" enctype="multipart/form-data">
            <input type="text" class="form-control" id="exampleInputEmail1" name="id" value="<%= temp['id']; %>" hidden>
            <input type="text" class="form-control" id="exampleInputEmail1" name="img" value="<%= temp['img']; %>" hidden>
    <div class="form-group">
      <label>rank</label>
      <input type="text" class="form-control" id="exampleInputEmail1" name="rank" value="<%= temp['rank']; %>">
    </div>
    <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" id="exampleInputPassword1" name="title" aria-describedby="emailHelp" value="<%= temp['title']; %>">
    </div>
    <div class="form-group">
      <label>Description</label>
      <textarea name="description" aria-describedby="emailHelp" class="form-control"><%= temp['description']; %></textarea>
    </div>
    <div class="form-group">
      <label>Image</label>
      <input name="img" type="file" class="form-control"></input>
    </div>
    <button type="submit"  class="btn btn-primary button submit-button" style="width: 100%;">Submit</button>
  </form>
        </div>
      </div>
    </div>
  </div>
<div class="card card-admin eventcard">
  <img class="card-img-top  e-image" src="/images/uploads/<%= temp['img']; %>" alt="Card image cap" style="object-fit:cover;">
   <div class="card-body cardstyle">
       <h5 class="card-title"><%= temp['title']; %></h5>
       rank:<%= temp['rank']; %>
      <p class="card-text"><%= temp['description']; %></p>
        <a class="btn btn-lg btn-primary button" data-toggle="modal" href="#contact_top<%= temp['id']; %>">Edit</a>  <a target="_blank" href="/cms/delete/contact_top/<%= temp['id'] %>" class="btn btn-lg btn-danger button">Delete</a>
  </div>
</div>

<%
  temp = [];
})
%>

</div>
<br>

